{
  "name": "sendit",
  "version": "1.0.0",
  "description": "SendIT is a courier service that helps users deliver parcels to different destinations.",
  "main": "server.js",
  "scripts": {
    "test": "mocha test/**/*.js || true",
    "start": "nodemon server.js --exec babel-node --presets env",
    "build": "babel server.js --out-dir build",
    "apidoc": "apidoc -f server.js -o public/apidoc"
  },
  "apidoc": {
    "name": "SendIT API - Documentation",
    "template": {
      "forceLanguage": "en"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bezalel001/SendIT.git"
  },
  "keywords": [
    "node",
    "express",
    "api",
    "postgresql"
  ],
  "author": "Michael Ogu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bezalel001/SendIT/issues"
  },
  "homepage": "https://github.com/bezalel001/SendIT#readme",
  "devDependencies": {
    "apidoc": "^0.17.7",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "make-runnable": "^1.3.6",
    "mocha": "^5.2.0",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.22.2",
    "nodemailer": "^4.6.8",
    "pg": "^7.6.1",
    "xoauth2": "^1.2.0"
  }
}
